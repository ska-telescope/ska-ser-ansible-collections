coder:
  env:
    - name: CODER_BROWSER_ONLY
      value: "true"
    - name: CODER_PG_CONNECTION_URL
      valueFrom:
        secretKeyRef:
          name: coder-db-url
          key: url

  service:
    enable: true
    type: LoadBalancer
    sessionAffinity: None

  ingress:
    # not possible to use a specific prefix
    enable: false

  serviceAccount:
    workspacePerms: false
